{
    "name": "nextdeveloper-nl/authentication",
    "type": "project",
    "description": "NextDeveloper agile and modular development platform",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
    ],
    "require": {
        "php": "^8.2.2",
        "guzzlehttp/guzzle": "*",
        "laravel/framework": "^9.19",
        "laravel/octane": "^1.4",
        "laravel/passport": "*",
        "laravel/tinker": "^2.7",
        "nextdeveloper/commons": "1.*",
        "nextdeveloper/generator": "1.*",
        "nextdeveloper/iam": "1.*",
        "nextdeveloper/i18n": "1.*",
        "spiral/roadrunner": "^2.8.2",
        "laravel/sanctum": "*",
        "beyondcode/laravel-websockets": "*",
        "predis/predis": "*",
        "laravel/socialite": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "*",
        "vimeo/psalm": "4.17.0",
        "friendsofphp/php-cs-fixer": ">=v2.19.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [

            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
